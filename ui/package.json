{
  "name": "dudo-ui",
  "version": "1.0.0",
  "description": "UI for Dudo at https://lefun.fun",
  "author": "Simon Lemieux",
  "license": "MIT",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/types/index.d.ts",
  "exports": {
    ".": "./dist/index.js",
    "./index.css": "./dist/index.css"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "vite:dev": "vite --host",
    "build": "pnpm run clean && pnpm run lingui:compile && pnpm run compile",
    "clean": "rm -rf ./dist",
    "compile": "pnpm rollup --config",
    "watch": "pnpm rollup --config --watch",
    "test": "pnpm vitest run src",
    "lingui:compile": "pnpm lingui compile",
    "lingui:extract": "pnpm lingui extract"
  },
  "devDependencies": {
    "@babel/preset-react": "^7.24.7",
    "@lefun/core": "^1.5.0",
    "@lefun/dev-server": "^1.5.0",
    "@lefun/game": "^1.5.0",
    "@lefun/ui": "^1.5.0",
    "@lefun/ui-testing": "^1.5.0",
    "@lingui/cli": "^4.11.1",
    "@lingui/conf": "^4.11.1",
    "@lingui/macro": "^4.11.1",
    "@lingui/react": "^4.11.1",
    "@lingui/vite-plugin": "^4.11.1",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-node-resolve": "^13.0.6",
    "@rollup/plugin-typescript": "^11.1.5",
    "@testing-library/react": "^16.0.0",
    "@types/react": "18.3.3",
    "@types/react-dom": "18.3.0",
    "@typescript-eslint/eslint-plugin": "^7.11.0",
    "@typescript-eslint/parser": "^7.11.0",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-simple-import-sort": "^12.1.0",
    "identity-obj-proxy": "^3.0.0",
    "jsdom": "^24.0.0",
    "postcss": "^8.4.1",
    "postcss-nested": "^5.0.6",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "rollup": "^2.60.1",
    "rollup-plugin-copy": "^3.5.0",
    "rollup-plugin-postcss": "^4.0.0",
    "tailwindcss": "^2.2.19",
    "tslib": "^2.3.1",
    "typescript": "^5.4.5",
    "vite": "^5.2.0",
    "vitest": "^1.2.1"
  },
  "dependencies": {
    "@react-spring/web": "^9.7.3",
    "@tippyjs/react": "^4.2.6",
    "classnames": "^2.3.1",
    "dudo-game": "workspace:*",
    "tippy.js": "^6.3.1"
  },
  "peerDependencies": {
    "@lefun/core": "*",
    "@lefun/game": "*",
    "@lefun/ui": "*",
    "react": ">=17.0.2",
    "react-dom": ">=17.0.2"
  }
}
